    "module-alias": "^2.2.3",
    "multer": "^2.0.2",
    "mysql2": "^3.16.2",
    "pdfkit": "^0.17.2",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
